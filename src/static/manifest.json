{
  "manifest_version": 3,
  "name": "Catalyst",
  "description": "Chrome Extension in React",
  "version": "1.0.0",
  "icons":{
    "16": "CatalystLogo_128.png",
    "48": "CatalystLogo_128.png",
    "128": "CatalystLogo_128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension1",
    "default_icon": "CatalystLogo_128.png"
  },
  "options_page": "options.html",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "42137827685-khoc4mfrhmaj59t3up0jcea6stm4cfgd.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "permissions":["storage", "alarms", "notifications", "identity", "identity.email", "contextMenus", "bookmarks", "search", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}